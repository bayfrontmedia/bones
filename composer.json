{
  "name": "bayfrontmedia/bones",
  "description": "A PHP framework used to build web apps using the MVC (model-view-controller) design pattern.",
  "homepage": "https://github.com/bayfrontmedia/bones",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "php",
    "framework",
    "mvc",
    "bones"
  ],
  "authors": [
    {
      "name": "John Robinson",
      "email": "john@bayfrontmedia.com",
      "homepage": "https://www.bayfrontmedia.com"
    }
  ],
  "support": {
    "issues": "https://github.com/bayfrontmedia/bones/issues"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist"
  },
  "autoload": {
    "psr-4": {
      "Bayfront\\Bones\\": "src/"
    }
  },
  "require": {
    "php": ">=7.2.0",
    "ext-json": "*",
    "ext-pdo": "*",
    "bayfrontmedia/container": "^1.1",
    "bayfrontmedia/cron-scheduler": "^1.1",
    "bayfrontmedia/filesystem-factory": "^1.2",
    "bayfrontmedia/monolog-factory": "^1.1",
    "bayfrontmedia/php-array-helpers": "^1.3",
    "bayfrontmedia/php-hooks": "^1.1",
    "bayfrontmedia/php-http-request": "^2.1",
    "bayfrontmedia/php-http-response": "^1.3",
    "bayfrontmedia/php-string-helpers": "^1.2",
    "bayfrontmedia/php-time-helpers": "^1.1",
    "bayfrontmedia/route-it": "^2.0",
    "bayfrontmedia/simple-pdo": "^1.0",
    "bayfrontmedia/translation": "^1.1",
    "bayfrontmedia/veil": "^1.3",
    "filp/whoops": "^2.14",
    "symfony/console": "^6.2",
    "vlucas/phpdotenv": "^5.3"
  }
}